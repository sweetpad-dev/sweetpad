      {
        "command": "sweetpad.build.refreshView",
        "title": "SweetPad: Refresh view",
        "icon": "$(sweetpad-refresh)"
      },
      {
        "command": "sweetpad.simulators.refresh",
        "title": "SweetPad: Refresh simulators list",
        "icon": "$(sweetpad-refresh)"
      },
      {
        "command": "sweetpad.simulators.openSimulator",
